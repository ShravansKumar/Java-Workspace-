package storingobjects;

class EmployeeMainClass {
	public static void main(String[] args) {
		Employee e1=new Employee("Sahala",21);
		Employee e2=new Employee("alvin",25);
		Employee e3=new Employee("Smith",50);

		Employee[] emp={e1,e2,e3};


		for(int i=0;i<emp.length;i++) {
			System.out.println(emp[i].age +" " +emp[i].name);
		}
	}
}
